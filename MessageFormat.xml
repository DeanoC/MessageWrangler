<filetype binary="false" description="Message Definition Format" name="MessageFormat">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="//" />
      <option name="COMMENT_START" value="/*" />
      <option name="COMMENT_END" value="*/" />
      <option name="HEX_PREFIX" value="" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_BRACKETS" value="true" />
      <option name="HAS_PARENS" value="true" />
      <option name="HAS_STRING_ESCAPES" value="true" />
    </options>
    <keywords keywords="enum;field;message;namespace;optional;default" ignore_case="false" />
    <keywords2 keywords="float;string;int;bool;byte;options" ignore_case="false" />
    <keywords3 keywords="FAIL;Failure;OK;Pending;Ping;Position;Success" ignore_case="false" />
    <keywords4 keywords=":;::" ignore_case="false" />
  </highlighting>
  <extensionMap>
    <mapping ext="def" />
  </extensionMap>
</filetype>
